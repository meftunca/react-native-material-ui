{
  "name": "react_native_variant",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "build": "npm run lint && npm run build-lib && build-storybook",
    "build-lib-watch": "tsc -w",
    "build-lib": "tsc && npm run copy-css-to-lib && npm run copy-svg-to-lib && npm run copy-png-to-lib && npm run copy-woff2-to-lib",
    "test": "react-app-rewired test --env=jsdom",
    "test:coverage": "npm test -- --coverage",
    "eject": "react-app-rewired eject",
    "storybook": "start-storybook -p 6006 -s ./public",
    "build-storybook": "build-storybook",
    "copy-css-to-lib": "cpx \"./src/**/*.css\" ./build/lib",
    "copy-woff2-to-lib": "cpx \"./src/**/*.woff2\" ./build/lib",
    "copy-svg-to-lib": "cpx \"./src/**/*.svg\" ./build/lib",
    "copy-png-to-lib": "cpx \"./src/**/*.png\" ./build/lib",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'"
  },
  "dependencies": {
    "@devloops/react-native-variant": "^0.2.1",
    "@react-native-community/masked-view": "^0.1.5",
    "@react-navigation/bottom-tabs": "^5.0.0-alpha.28",
    "@react-navigation/core": "^5.0.0-alpha.29",
    "@react-navigation/drawer": "^5.0.0-alpha.30",
    "@react-navigation/native": "^5.0.0-alpha.21",
    "@react-navigation/stack": "^5.0.0-alpha.46",
    "color": "^3.1.2",
    "react": "^16.12.0",
    "react-art": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-native": "0.61.5",
    "react-native-gesture-handler": "^1.5.2",
    "react-native-reanimated": "^1.4.0",
    "react-native-safe-area-context": "^0.6.1",
    "react-native-safe-area-view": "^1.0.0",
    "react-native-screens": "^2.0.0-alpha.22",
    "react-native-vector-icons": "^6.6.0",
    "react-native-web": "^0.12.0-rc.1",
    "react-scripts": "^3.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/plugin-transform-modules-commonjs": "^7.7.5",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-flow": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "@babel/runtime": "^7.7.7",
    "@react-native-community/eslint-config": "^0.0.5",
    "@react-navigation/web": "^1.0.0-alpha.9",
    "@storybook/addon-actions": "^5.3.0-rc.0",
    "@storybook/addon-docs": "^5.3.0-rc.0",
    "@storybook/addon-info": "^5.3.0-rc.0",
    "@storybook/addon-knobs": "^5.3.0-rc.0",
    "@storybook/addon-links": "^5.3.0-rc.0",
    "@storybook/addon-options": "^5.3.0-rc.0",
    "@storybook/addon-storysource": "^5.3.0-rc.0",
    "@storybook/addon-viewport": "^5.3.0-rc.0",
    "@storybook/addons": "^5.3.0-rc.0",
    "@storybook/preset-create-react-app": "^1.5.0",
    "@storybook/preset-typescript": "^1.2.0",
    "@storybook/react": "^5.3.0-rc.0",
    "@storybook/theming": "^5.3.0-rc.0",
    "@types/jest": "^24.0.24",
    "@types/react": "^16.9.17",
    "@types/react-native": "^0.60.25",
    "@types/react-native-vector-icons": "^6.4.5",
    "@types/react-test-renderer": "16.9.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-react-native-web": "^0.12.0-rc.1",
    "core-js": "3.6.0",
    "css-loader": "^3.4.0",
    "detox": "^14.9.1",
    "file-loader": "^5.0.2",
    "fork-ts-checker-webpack-plugin": "^4.0.0-beta.4",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.57.0",
    "module-alias": "^2.2.2",
    "node-sass": "^4.13.0",
    "react-app-rewired": "^2.1.5",
    "react-docgen-typescript-loader": "^3.6.0",
    "react-is": "^16.12.0",
    "react-test-renderer": "16.12.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.1.1",
    "ts-jest": "^24.2.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.4",
    "url-loader": "^3.0.0"
  },
  "_moduleAliases": {
    "@devloops/rn-mui": "./../src"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "@react-native-community/bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "commonjs",
        {
          "flow": true
        }
      ],
      "module",
      "typescript"
    ]
  },
  "detox": {
    "test-runner": "jest"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
