{
  "name": "react_native_variant",
  "version": "0.0.1",
  "private": false,
  "scripts": {
    "start": "react-scripts-ts start",
    "build-with-lint": "npm run lint && npm run build-lib && build-storybook",
    "build": "npm run build-lib && build-storybook",
    "build-lib-watch": "tsc -w",
    "build-lib": "tsc ",
    "test": "react-scripts-ts test --env=jsdom",
    "test:coverage": "npm test -- --coverage",
    "eject": "react-scripts-ts eject",
    "storybook": "NODE_PATH=src start-storybook -p 6006 -s ./docs",
    "build-storybook": "build-storybook -c .storybook -o ./../docs",
    "copy-css-to-lib": "cpx \"./public/**/*.css\" ./build/lib",
    "copy-woff2-to-lib": "cpx \"./public/**/*.woff2\" ./build/lib",
    "copy-svg-to-lib": "cpx \"./public/**/*.svg\" ./build/lib",
    "copy-png-to-lib": "cpx \"./public/*.png\" ./build && cpx \"./public/*.ico\" ./build",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'"
  },
  "dependencies": {
    "@callstack/react-theme-provider": "^3.0.5",
    "@devloops/react-native-variant": "^0.3.7",
    "color": "^3.1.2",
    "core-js": "3.6.0",
    "react": "^16.12.0",
    "react-art": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-native": "0.61.5",
    "react-native-gesture-handler": "^1.5.2",
    "react-native-reanimated": "^1.4.0",
    "react-native-safe-area-context": "^0.6.1",
    "react-native-safe-area-view": "^1.0.0",
    "react-native-screens": "^2.0.0-alpha.22",
    "react-native-vector-icons": "^6.6.0",
    "react-native-web": "^0.12.0-rc.1",
    "react-scripts": "^3.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/plugin-transform-classes": "^7.7.4",
    "@babel/plugin-transform-modules-commonjs": "^7.7.5",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-flow": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "@babel/runtime": "^7.7.7",
    "@mdx-js/loader": "^1.5.3",
    "@react-native-community/eslint-config": "^0.0.5",
    "@storybook/addon-docs": "^5.3.0-rc.4",
    "@storybook/addon-info": "^5.3.0-rc.4",
    "@storybook/addon-knobs": "^5.2.8",
    "@storybook/addon-links": "^5.3.0-rc.4",
    "@storybook/addon-actions": "^5.3.0-rc.4",
    "@storybook/addon-options": "^5.3.0-rc.4",
    "@storybook/addon-viewport": "^5.3.0-rc.4",
    "@storybook/addons": "^5.3.0-rc.4",
    "@storybook/preset-create-react-app": "^1.5.0",
    "@storybook/preset-typescript": "^1.2.0",
    "@storybook/react": "^5.3.0-rc.4",
    "@storybook/theming": "^5.3.0-rc.4",
    "@types/jest": "^24.0.25",
    "@types/react": "^16.9.17",
    "@types/react-native": "^0.60.26",
    "@types/react-native-vector-icons": "^6.4.5",
    "@types/react-test-renderer": "16.9.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-react-native-web": "^0.12.0-rc.1",
    "css-loader": "^3.4.0",
    "file-loader": "^5.0.2",
    "fork-ts-checker-webpack-plugin": "^4.0.0-beta.4",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.57.0",
    "module-alias": "^2.2.2",
    "node-sass": "^4.13.0",
    "raw-loader": "^4.0.0",
    "react-app-rewired": "^2.1.5",
    "react-docgen-typescript-loader": "^3.6.0",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "react-is": "^16.12.0",
    "react-test-renderer": "16.12.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.1.2",
    "ts-jest": "^24.2.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.4",
    "url-loader": "^3.0.0"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
